(program 
   (print "--- literal values ---")
   (print "")
   (print "string")
   (print 0)
   (print 3.14)
   (print 42)
   (print true)
   (print false)
   (print nil)
   (print "")
   (print "--- truthiness ---")
   (print (+ "\"\": " (! (! ""))))
   (print (+ "\"string\": " (! (! "string"))))
   (print (+ "42: " (! (! 42))))
   (print (+ "0: " (! (! 0))))
   (print (+ "3.14: " (! (! 3.14))))
   (print (+ "0.00: " (! (! 0.00))))
   (print (+ "true: " (! (! true))))
   (print (+ "false: " (! (! false))))
   (print (+ "nil: " (! (! nil))))
)
