(program 
   (class Breakfast
      (fun cook (let [] (block 
         (print "Fried eggs.")
      )))
      (fun serve (let [who] (block 
         (print (+ (+ "Enjoy, " who) "."))
      )))
   )
   (class Bagel
      
   )
   (var bagel (call Bagel))
   (.set bagel isReady false)
   (print (?: (.get bagel isReady) "Bagel is ready" "... still toasting..."))
   (.set bagel isReady true)
   (print (?: (.get bagel isReady) "Bagel is ready" "... still toasting..."))
)
